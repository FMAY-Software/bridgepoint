<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.xtuml.bp.ui.graphics.editorTab">
      <EditorTab
            EditorTabFactory="org.xtuml.bp.core.editors.focus.factories.MetamodelTabFactory"
            EditorTitle="Tree Editor"
            Input="org.xtuml.bp.core.common.NonRootModelElement">
      </EditorTab>
      <EditorTab
            EditorTabFactory="org.xtuml.bp.core.editors.association.factories.AssociationEditorTabFactory"
            EditorTitle="Association Editor"
            Input="org.xtuml.bp.core.Association_c"
            SelectionInput="org.xtuml.bp.core.Association_c"
            childObject="org.xtuml.bp.core.Association_c"
            focusBased="true"
            selectionInput="org.xtuml.bp.core.Association_c">
      </EditorTab>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.xtuml.bp.core.editors.ModelEditor"
            default="false"
            icon="icons/search.png"
            id="org.xtuml.bp.core.editors.ModelEditor"
            name="Focus Editor">
      </editor>
   </extension>
</plugin>
