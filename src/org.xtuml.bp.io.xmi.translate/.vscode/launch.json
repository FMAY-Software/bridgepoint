  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch XMITranslator",
      "request": "launch",
      "mainClass": "org.xtuml.bp.io.xmi.translate.XMITranslator",
      "projectName": "org.xtuml.bp.io.xmi.translate",
      "args": [
        "-mm",
        "${workspaceFolder}/src/main/resources/metamodel2.xml",
        "-t",
        "${workspaceFolder}/src/main/resources/xmiTrans2_0.xml",
        "-t",
        "${workspaceFolder}/src/main/resources/xtumlExtensionsTrans.xml",
        "-x",
        "${workspaceFolder}/src/main/resources/messages.xml",
        "-todo",
        "-v",
        "-overwrite",
        "-out",
        "${workspaceFolder}/output/test.xtuml"
      ]
    }
  ]
}
